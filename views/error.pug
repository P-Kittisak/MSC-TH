    title= 'หาหน้าไม่พบ | MSC TH'
    meta(charset='UTF-8')
    meta(name='description', content='')
    // Fonts
    style.
      @import url('/fonts/font-awesome-4.7.0/fonts/Kanit.css');
      body{
        font-family: 'Kanit', sans-serif !important;
      }
    // Icons. Uncomment required icon fonts
    link(rel='stylesheet', href='/shop/assets/vendor/fonts/boxicons.css')
    link(rel='icon' type='image/png' href='/images/icons/Logo-msc-mini.png')
    // Core CSS
    link.template-customizer-core-css(rel='stylesheet', href='/shop/assets/vendor/css/core.css')
    link.template-customizer-theme-css(rel='stylesheet', href='/shop/assets/vendor/css/theme-default.css')
    link(rel='stylesheet', href='/shop/assets/css/demo.css')
    // Vendors CSS
    link(rel='stylesheet', href='/shop/assets/vendor/libs/perfect-scrollbar/perfect-scrollbar.css')
    // Page CSS
